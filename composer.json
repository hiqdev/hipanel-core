        "hiqdev/yii2-hiam-authclient": "dev-master",
        "hiqdev/yii2-combo": "dev-master",
        "hiqdev/yii2-export": "dev-master",
        "hiqdev/yii2-higrid": "*@dev",
        "hiqdev/yii2-language": "*@dev",
